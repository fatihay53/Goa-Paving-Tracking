{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
  ],
  "plugins": [
    "react",
    "react-hooks"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "requireConfigFile": false,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true
    }
  }
}
